import java.util.*;
 
class Num
{
    public static void main(String []s)
    {
        int n;
        Scanner sc=new Scanner(System.in);
         
        System.out.print("Enter any integer number: ");
        n=sc.nextInt();
        if(n%2==0)
            System.out.println( "The number is even");
        else
            System.out.println("The Number is not even");
         
    }
}
