import java.util.*;
 
class Num
{
    public static void main(String []s)
    {
        int n;
        Scanner sc=new Scanner(System.in);
         
        System.out.print("Enter any integer number which is more than one digit: ");
        n=sc.nextInt();
        if (n>9)
            System.out.println(n%100);
        else if((n<10)&&(n>-10))
            System.out.println("it is a onedigit number");
         
    }
}
